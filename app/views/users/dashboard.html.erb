<div class="index-bar-beer">
  <h1 class="beer-name">
    <i class="fa fa-tachometer" aria-hidden="true"></i> Dashboard
  </h1>
</div>

<h1>My Wishlists</h1>

<% @my_wishes.each do |wishlist| %>
<p><%= wishlist.user.first_name %> <%= wishlist.user.last_name %> <%= wishlist.beer.name %> Quantity : <%= wishlist.quantity %></p>
<% end %>

<h1>My beers to send away</h1>
<% @my_beers.each do |beer| %>
  <p><%= beer.name %></p>
  <% beer.wishlists.each do |wishlist| %>
  <p><%= wishlist.user.first_name %> <%= wishlist.user.last_name %> </p>
  <p><%= wishlist.user.address %></p>
  <p><%= wishlist.user.cp %> <%= wishlist.user.city %> (<%= wishlist.user.country %>)</p>
  <% end %>
<% end %>

